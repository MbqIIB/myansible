  605  2016-11-15 03:46:58 #!/bin/bash
  606  2016-11-15 03:46:58 #
  607  2016-11-15 03:46:58 # Script to make a proxy (ie HAProxy) capable of monitoring Percona XtraDB Cluster nodes properly
  608  2016-11-15 03:46:58 #
  609  2016-11-15 03:46:58 # Author: Olaf van Zandwijk <olaf.vanzandwijk@nedap.com>
  610  2016-11-15 03:46:58 # Author: Raghavendra Prabhu <raghavendra.prabhu@percona.com>
  611  2016-11-15 03:46:58 #
  612  2016-11-15 03:46:58 # Documentation and download: https://github.com/olafz/percona-clustercheck
  613  2016-11-15 03:46:58 #
  614  2016-11-15 03:46:58 # Based on the original script from Unai Rodriguez
  615  2016-11-15 03:46:58 #
  616  2016-11-15 03:46:58 if [[ $1 == '-h' || $1 == '--help' ]];then     echo "Usage: $0 <user> <pass> <available_when_donor=0|1> <log_file> <available_when_readonly=0|1> <defaults_extra_file>";     exit; fi
  617  2016-11-15 03:46:58 # if the disabled file is present, return 503. This allows
  618  2016-11-15 03:46:58 # admins to manually remove a node from a cluster easily.
  619  2016-11-15 03:46:58 if [ -e "/var/tmp/clustercheck.disabled" ]; then     echo -en "HTTP/1.1 503 Service Unavailable\r\n";     echo -en "Content-Type: text/plain\r\n";     echo -en "Connection: close\r\n";     echo -en "Content-Length: 51\r\n";     echo -en "\r\n";     echo -en "Percona XtraDB Cluster Node is manually disabled.\r\n";     sleep 0.1;     exit 1; fi
  620  2016-11-15 03:46:58 MYSQL_USERNAME="${1-clustercheckuser}"
  621  2016-11-15 03:46:58 AVAILABLE_WHEN_DONOR=${3:-0}
  622  2016-11-15 03:46:58 ERR_FILE="${4:-/dev/null}"
  623  2016-11-15 03:46:58 AVAILABLE_WHEN_READONLY=${5:-1}
  624  2016-11-15 03:46:58 DEFAULTS_EXTRA_FILE=${6:-/etc/my.cnf}
  625  2016-11-15 03:46:58 #Timeout exists for instances where mysqld may be hung
  626  2016-11-15 03:46:58 TIMEOUT=10
  627  2016-11-15 03:46:58 EXTRA_ARGS=""
  628  2016-11-15 03:46:58 if [[ -n "$MYSQL_USERNAME" ]]; then     EXTRA_ARGS="$EXTRA_ARGS --user=${MYSQL_USERNAME}"; fi
  629  2016-11-15 03:46:58 if [[ -n "$MYSQL_PASSWORD" ]]; then     EXTRA_ARGS="$EXTRA_ARGS --password=${MYSQL_PASSWORD}"; fi
  630  2016-11-15 03:46:58 if [[ -r $DEFAULTS_EXTRA_FILE ]];then     MYSQL_CMDLINE="mysql --defaults-extra-file=$DEFAULTS_EXTRA_FILE -nNE --connect-timeout=$TIMEOUT \
  631  2016-11-15 03:46:58                     ${EXTRA_ARGS}"; else     MYSQL_CMDLINE="mysql -nNE --connect-timeout=$TIMEOUT ${EXTRA_ARGS}"; fi
  632  2016-11-15 03:46:58 #
  633  2016-11-15 03:46:58 # Perform the query to check the wsrep_local_state
  634  2016-11-15 03:46:58 #
  635  2016-11-15 03:46:58 WSREP_STATUS=$($MYSQL_CMDLINE -e "SHOW STATUS LIKE 'wsrep_local_state';" \
  636  2016-11-15 03:46:58     2>${ERR_FILE} | tail -1 2>>${ERR_FILE})
  637  2016-11-15 03:46:58 if [[ "${WSREP_STATUS}" == "4" ]] || [[ "${WSREP_STATUS}" == "2" && ${AVAILABLE_WHEN_DONOR} == 1 ]]; then     if [[ $AVAILABLE_WHEN_READONLY -eq 0 ]];then         READ_ONLY=$($MYSQL_CMDLINE -e "SHOW GLOBAL VARIABLES LIKE 'read_only';" \
  638  2016-11-15 03:46:58                     2>${ERR_FILE} | tail -1 2>>${ERR_FILE});          if [[ "${READ_ONLY}" == "ON" ]];then             echo -en "HTTP/1.1 503 Service Unavailable\r\n"
  639  2016-11-15 03:46:58 vim /etc/haproxy/haproxy.cfg
  640  2016-11-15 03:46:58 mysql-check user glance
  641  2016-11-15 03:46:58 mysqlcheck user glance
  642  2016-11-15 03:46:58 mysql -uroot -pdemopass
  643  2016-11-15 03:46:58 pcs resoruce
  644  2016-11-15 03:46:58 netstat -natp|grep 9200
  645  2016-11-15 03:46:58 vim /etc/glance/glance-api.conf 
  646  2016-11-15 03:46:58 netstat -ntpa | grep 9200
  647  2016-11-15 03:46:58 vim /etc/haproxy/haproxy.cfg
  648  2016-11-15 03:46:58 mysql -uroot -pdemopass
  649  2016-11-15 03:46:58 vim /etc/haproxy/haproxy.cfg
  650  2016-11-15 03:46:58 ls
  651  2016-11-15 03:46:58 cd scripts/
  652  2016-11-15 03:46:58 ls
  653  2016-11-15 03:46:58 vim setupctl.sh 
  654  2016-11-15 03:46:58 scp setupctl.sh poc-ctl1:/root/scripts/
  655  2016-11-15 03:46:58 scp setupctl.sh poc-ctl2:/root/scripts/
  656  2016-11-15 03:46:58 pcs resource
